{
    "name": "azine/emailupdateconfirmation-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to require the user to confirm her update email address by clicking on a link sent to the new address. This should be part of the FriendsOfSymfony/UserBundle, but was not accepted as new feature by a maintainer.",
    "keywords": ["email", "update", "confirm", "email update confirmation"],
    "homepage": "https://github.com/azine/AzineEmailUpdateConfirmationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Azine IT Services AG",
            "email": "github@azine-it.ch"
        }
    ],
    "config": {
        "process-timeout": 590,
        "github-protocols": ["https", "git", "ssh"],
        "github-domains": ["github.com"]
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/http-foundation": "^2.7|^3.0|^4.0",
        "symfony/event-dispatcher": "^2.7|^3.0|^4.0",
        "symfony/translation": "^2.7|^3.0|^4.0",
        "symfony/routing": "^2.7|^3.0|^4.0",
        "symfony/validator": "^2.7|^3.0|^4.0",
        "symfony/yaml": "^2.7|^3.0|^4.0",
        "friendsofsymfony/user-bundle": "^2.0",
        "monolog/monolog": "^1.6,>=1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit":  "^5.7.27|^6.0|^7.0",
        "friendsofphp/php-cs-fixer": "^2.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": { "Azine\\EmailUpdateConfirmationBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Azine/EmailUpdateConfirmationBundle"
}
